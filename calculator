import math
# Ввід чисел
a = float(input('Введи перше число:' ))
b = float(input(' Введи друге число :' ))
# Вибираєм знак
symbol = (input('+,-,*,/?:' ))
if symbol == '+':
    c = a + b 
    print(c)
elif symbol == '-':
    c = a - b 
    print(c)
elif symbol == '*':
    c = a * b 
    print(c)
elif symbol == '/':
    c = a / b 
    print(c)
else:
    print('Введено невірне значення')        
